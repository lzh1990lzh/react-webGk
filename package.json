    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^0.9.10",
    "@ant-design/pro-layout": "^5.0.12",
    "@types/echarts": "^4.6.5",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.14",
    "antd": "^4.6.0",
    "classnames": "^2.2.6",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "eslint": "^7.6.0",
    "lint-staged": "^10.0.7",
    "md5": "^2.3.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "three": "^0.120.1",
    "three-css2drender": "^1.0.0",
    "three-obj-mtl-loader": "^1.0.3",
    "three-orbit-controls": "^82.1.0",
    "umi": "^3.2.14",
    "umi-request": "^1.3.5",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {}
